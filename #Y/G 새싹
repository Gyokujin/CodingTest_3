#include <iostream>
using namespace std;

// 아래 예제와 같이 새싹을 출력하시오.
//          ,r'"7
// r`-_   ,'  ,/
//  \. ". L_r'
//    `~\/
//       |
//       |

int main()
{
	cout << "         ,r'\"7" << endl;
	cout << "r`-_   ,'  ,/" << endl;
	cout << " \\. \". L_r'" << endl;
	cout << "   `~\\/" << endl;
	cout << "      |" << endl;
	cout << "      |" << endl;
}
