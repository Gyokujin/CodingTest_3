using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class Test245 : MonoBehaviour
{
    // 정수 num1과 num2가 매개변수로 주어집니다. 두 수가 같으면 1 다르면 -1을 retrun하도록 solution 함수를 완성해주세요.
    
    void Start()
    {
        Solution(2, 3);
        Solution(11, 11);
        Solution(7, 99);
        Solution(1, -1);
        Solution(-4, -4);
    }

    int Solution(int num1, int num2)
    {
        int correct = num1 == num2 ? 1 : -1;
        Debug.Log(correct);
        return correct;
    }
}
