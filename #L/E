using System;

public class Example
{
    // 정수 a와 b가 주어집니다. 각 수를 입력받아 입출력 예와 같은 형식으로 출력하는 코드를 작성해 보세요.

    public static void Main()
    {
        String[] inputs = Console.ReadLine().Split(" ");
        Console.WriteLine($"a = {inputs[0]}");
        Console.WriteLine($"b = {inputs[1]}");
    }
}
