using System;

namespace ConsoleApp2
{
    class Program
    {
        // 정수 n이 주어질 때, n을 문자열로 변환하여 return하도록 solution 함수를 완성해주세요.

        static void Main(string[] args)
        {
            Solution(123);
            Solution(2573);
        }

        public static string Solution(int n)
        {
            string correct = n.ToString();
            return correct;
        }
    }
}
