using System;

public class Solution {
    public string solution(string rny_string) {
        string answer = "";
        
        for (int i = 0; i < rny_string.Length; i++){
            string s = rny_string[i].ToString();
            
            if (s == "m")
                answer += "rn";
            else
                answer += s;
        }
        
        return answer;
    }
}
