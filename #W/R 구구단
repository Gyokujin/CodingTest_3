#include <iostream>
using namespace std;

// N을 입력받은 뒤, 구구단 N단을 출력하는 프로그램을 작성하시오. 출력 형식에 맞춰서 출력하면 된다.
int main()
{
	int input;
	cin >> input;

	cout << input << " * 1 = " << input * 1 << endl;
	cout << input << " * 2 = " << input * 2 << endl;
	cout << input << " * 3 = " << input * 3 << endl;
	cout << input << " * 4 = " << input * 4 << endl;
	cout << input << " * 5 = " << input * 5 << endl;
	cout << input << " * 6 = " << input * 6 << endl;
	cout << input << " * 7 = " << input * 7 << endl;
	cout << input << " * 8 = " << input * 8 << endl;
	cout << input << " * 9 = " << input * 9 << endl;
}
